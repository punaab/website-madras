{
  "name": "madras",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "prisma generate --schema=prisma/schema.development.prisma && next dev",
    "build": "prisma generate --schema=prisma/schema.production.prisma && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:push": "prisma db push --schema=prisma/schema.development.prisma",
    "db:push:prod": "prisma db push --schema=prisma/schema.production.prisma",
    "db:seed": "node prisma/seed.js",
    "db:studio": "prisma studio --schema=prisma/schema.development.prisma"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.12",
    "@heroicons/react": "^2.1.1",
    "@prisma/client": "^5.7.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tiptap/extension-color": "^2.2.4",
    "@tiptap/extension-highlight": "^2.2.4",
    "@tiptap/extension-image": "^2.2.4",
    "@tiptap/extension-link": "^2.2.4",
    "@tiptap/extension-placeholder": "^2.2.4",
    "@tiptap/extension-subscript": "^2.2.4",
    "@tiptap/extension-superscript": "^2.2.4",
    "@tiptap/extension-text-align": "^2.2.4",
    "@tiptap/extension-text-style": "^2.2.4",
    "@tiptap/extension-underline": "^2.2.4",
    "@tiptap/react": "^2.2.4",
    "@tiptap/starter-kit": "^2.2.4",
    "@tinymce/tinymce-react": "^4.3.2",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dotenv": "^16.5.0",
    "lucide-react": "^0.344.0",
    "next": "14.0.4",
    "next-auth": "^4.24.5",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "prisma": "^5.7.1",
    "tailwindcss": "^3.3.0"
  }
}
