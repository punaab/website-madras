{
  "name": "madras-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate --schema=prisma/schema.production.prisma && next build",
    "start": "prisma db push --schema=prisma/schema.production.prisma --force-reset --accept-data-loss && next start",
    "lint": "next lint",
    "db:push": "prisma db push",
    "db:push:prod": "prisma db push --schema=prisma/schema.production.prisma",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "setup": "prisma generate --schema=prisma/schema.production.prisma && prisma migrate deploy --schema=prisma/schema.production.prisma && prisma db seed"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.1",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.9.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tinymce/tinymce-react": "^6.2.1",
    "@tiptap/extension-highlight": "^2.12.0",
    "@tiptap/extension-image": "^2.12.0",
    "@tiptap/extension-link": "^2.12.0",
    "@tiptap/extension-placeholder": "^2.12.0",
    "@tiptap/extension-subscript": "^2.12.0",
    "@tiptap/extension-superscript": "^2.12.0",
    "@tiptap/extension-text-align": "^2.12.0",
    "@tiptap/extension-underline": "^2.12.0",
    "@tiptap/pm": "^2.12.0",
    "@tiptap/react": "^2.12.0",
    "@tiptap/starter-kit": "^2.12.0",
    "@types/bcryptjs": "^2.4.6",
    "autoprefixer": "^10.4.17",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "lucide-react": "^0.513.0",
    "next": "14.1.0",
    "next-auth": "^4.24.11",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "prisma": "^6.9.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
